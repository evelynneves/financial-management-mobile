{
  "expo": {
    "name": "financial-management-mobile",
    "slug": "financial-management-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "financialmanagementmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#004D61"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.financialmanagementmobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#004D61"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
